{
  "name": "since 6month ago",
  "version": "1.0.1",
  "manifest_version": 3,
  "description": "Activeにしている間、Google検索結果を6ヶ月以内のものに限定します。",
  "background": {
    "page": "app/background/background.html"
  },
  "browser_action": {
    "default_icon": "app/icon/icon-19-off.png",
    "default_title": "since 6month ago"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "app/icon/icon-128.png"
  }
}
