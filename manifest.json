{
  "name": "search in 6 months ",
  "version": "1.2.0",
  "manifest_version": 3,
  "description": "When Active, the search results are since 6 months old.",
  "icons": {
    "16": "./img/icon/white_6/16.png",
    "48": "./img/icon/white_6/48.png",
    "128": "./img/icon/white_6/128.png"
  },
  "action": {
    "default_icon": "./img/icon/white_6/128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "tabs"
  ]
}
